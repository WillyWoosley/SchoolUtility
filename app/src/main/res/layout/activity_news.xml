<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gamecodeschool.schoolutility.NewsActivity">

    <WebView
        android:id="@+id/newsWebView"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
    />


    <fragment
        android:name="com.gamecodeschool.schoolutility.MenubarFragment"
        android:id="@+id/newsMenubar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        tools:layout="@layout/menubar_fragment" />

</RelativeLayout>
